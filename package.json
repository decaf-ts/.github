{
  "name": "@decaf-ts/decaf-ts",
  "version": "0.0.1",
  "description": "Readme page for decaf-ts project",
  "scripts": {
    "build:banner": "gs -dNOPAUSE -dBATCH -sDEVICE=png16m -r300 -sOutputFile=resources/banner.png assets/illustrator/banner.ai",
    "build:cover": "gs -dNOPAUSE -dBATCH -sDEVICE=png16m -r300 -sOutputFile=resources/cover.png assets/illustrator/cover.ai",
    "build:icon": "gs -dNOPAUSE -dBATCH -sDEVICE=png16m -r300 -sOutputFile=resources/icon.png assets/illustrator/icon.ai",
    "build": "npm run build:banner && npm run build:cover && npm run build:icon",
    "docs": "npm run build && npx markdown-include ./mdCompile.json"
  },
  "files": [
    "dist/",
    "lib/"
  ],
  "licence": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decaf-ts/decorator-validation.git"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "decaf-ts",
    "validation",
    "cross-platorm",
    "customization",
    "open-source",
    "decorators",
    "typescript",
    "custom validation"
  ],
  "author": "Tiago Venceslau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/decaf-ts/decorator-validation/issues"
  },
  "homepage": "https://github.com/decaf-ts/decorator-validation#readme",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-run-command": "^0.0.10",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-junit": "^16.0.0",
    "jsdoc": "^4.0.2",
    "jsdoc-mermaid": "^1.0.0",
    "markdown-include": "^0.4.3",
    "merge-stream": "^2.0.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
